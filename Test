    table.insert(teleportPositions, pos)
end

local canTeleport = true

RunService.RenderStepped:Connect(function()
    local currentPosition = rootPart.Position
    local distance = (currentPosition - targetPosition).Magnitude

    if distance <= detectionRadius and canTeleport then
        canTeleport = false
        print("Player near target! Waiting 5 seconds before teleport chain...")

        task.delay(5, function()
            for _, pos in ipairs(teleportPositions) do
                rootPart.CFrame = CFrame.new(pos)
                task.wait(0.02)
            end
            print("Teleportation chain complete!")
            wait(cooldownTime)  -- cooldown before next possible teleport
            canTeleport = true
        end)
    end
end)
